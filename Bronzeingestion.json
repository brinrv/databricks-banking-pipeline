{
  "name": "Bronzeingestion",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "parameters",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Project_bank/srcParameters",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "incremental_ingestion",
      "depends_on": [
        {
          "task_key": "parameters"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "{{tasks.parameters.values.output_key}}",
        "task": {
          "task_key": "incremental_ingestion_iteration",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "/Workspace/Project_bank/Bronze_layer",
            "base_parameters": {
              "src": "{{input.src}}"
            },
            "source": "WORKSPACE"
          },
          "timeout_seconds": 0,
          "email_notifications": {},
          "webhook_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Silver_layer",
      "depends_on": [
        {
          "task_key": "incremental_ingestion"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Project_bank/silver_notebook",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Gold_layer",
      "depends_on": [
        {
          "task_key": "Silver_layer"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Project_bank/Gold_layer",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    }
  ],
  "queue": {
    "enabled": true
  },
  "performance_target": "STANDARD",
  "run_as": {
    "user_name": "vbirundha121@gmail.com"
  }
}